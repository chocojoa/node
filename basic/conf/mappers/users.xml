<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="users">
    
    <select id="selectUserList">
        SELECT ID
             , USER_ID
             , USER_NAME
             , EMAIL
             , PASSWORD
             , CREATED_DATE
             , CREATED_BY 
          FROM users 
         WHERE EMAIL = #{email}
    </select>

</mapper>